        <script>
            $('#op_reklame_nm').typeahead({
                 "hint"     : true,
                 "highlight": true,
                 "minLength": 1,
				 "limit"    : 10,
                 "remote"   : "/reklame/opreklame/hon_op/act?term=%QUERY",
            },{
                "name"      : 'op_reklame_nm',
                "displayKey": 'value',
            });

            $('#op_reklame_nm').bind('typeahead:selected', function(obj, datum, name) {      
                  $('#op_reklame_id').val(datum.id);
                  $('#jumlah_op').val(datum.jml);
            });
